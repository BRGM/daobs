  </harvester>
  <harvester>
    <territory>pl</territory>
    <folder>pl</folder>
    <name>Geoportal - Polska Usługa WyszukiwaniaINSPIRE</name>
    <url>http://mapy.geoportal.gov.pl/wss/service/CSWINSP/guest/CSWStartup</url>
