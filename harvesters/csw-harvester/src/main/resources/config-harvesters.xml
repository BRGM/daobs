<harvesters>
  <!-- List of CSW endpoints
  from http://inspire-geoportal.ec.europa.eu/INSPIRERegistry/ -->

  <harvester>
    <territory>fr</territory>
    <folder>fr</folder>
    <name>Geocatalogue Catalogue Server INSPIRE</name>
    <url>http://www.geocatalogue.fr/api-public/inspire/servicesRest?</url>
  </harvester>
  <harvester>
    <territory>es</territory>
    <folder>es</folder>
    <name>IDEE Discovery service</name>
    <url>http://www.idee.es/csw-inspire-idee/srv/spa/csw</url>
    <filter>
      <ogc:Filter xmlns:ogc="http://www.opengis.net/ogc">
        <ogc:Or>
          <ogc:PropertyIsEqualTo>
            <ogc:PropertyName>apiso:type</ogc:PropertyName>
            <ogc:Literal>series</ogc:Literal>
          </ogc:PropertyIsEqualTo>
          <ogc:PropertyIsEqualTo>
            <ogc:PropertyName>apiso:type</ogc:PropertyName>
            <ogc:Literal>service</ogc:Literal>
          </ogc:PropertyIsEqualTo>
          <ogc:PropertyIsEqualTo>
            <ogc:PropertyName>apiso:type</ogc:PropertyName>
            <ogc:Literal>dataset</ogc:Literal>
          </ogc:PropertyIsEqualTo>
        </ogc:Or>
      </ogc:Filter>
    </filter>
  </harvester>
  <harvester>
    <territory>fi</territory>
    <folder>fi</folder>
    <name>Paikkatietohakemisto</name>
    <url>http://www.paikkatietohakemisto.fi/geonetwork/srv/fi/csw</url>
  </harvester>
  <harvester>
    <territory>fr</territory>
    <folder>fr</folder>
    <name>Geocatalogue Catalogue Server INSPIRE</name>
    <url>http://www.geocatalogue.fr/api-public/inspire/servicesRest?</url>
  </harvester>
  <harvester>
    <territory>hr</territory>
    <folder>hr</folder>
    <name>Pilot kataloškog servera</name>
    <url>http://geoportal.nipp.hr/geonetwork/srv/hrv/csw-inspire</url>
  </harvester>
  <harvester>
    <territory>si</territory>
    <folder>si</folder>
    <name>Slovenski Inspire metadata katalog</name>
    <url>http://prostor3.gov.si/imps/srv/sl/csw</url>
  </harvester>
  <harvester>
    <territory>sk</territory>
    <folder>sk</folder>
    <name>Katalógová (vyhľadávacia) služba MŽP SR</name>
    <url>http://geo.enviroportal.sk/catalog-server/CSWStartup</url>
  </harvester>

  <harvester>
    <territory>ee</territory>
    <folder>ee</folder>
    <name>Estonia Discovery Service</name>
    <url>http://inspire.maaamet.ee/geoportal/csw</url>
  </harvester>
  <harvester>
    <territory>el</territory>
    <folder>el</folder>
    <name>ΚΑΤΑΛΟΓΟΣ</name>
    <url>http://www1.okxe.gr/geonetwork/srv/en/csw</url>
  </harvester>
  <harvester>
    <territory>lv</territory>
    <folder>lv/lgia</folder>
    <name>Latvian Geospatial Information Agency metadata catalogue</name>
    <url>http://kartes.lgia.gov.lv/geoportal/csw</url>
  </harvester>
  <harvester>
    <territory>lv</territory>
    <folder>lv/viss</folder>
    <name>Latvian metadata catalogue. GDC (Geospatial data connector)</name>
    <url>https://geometadati.viss.gov.lv/geoportal/csw</url>
  </harvester>
  <harvester>
    <territory>at</territory>
    <folder>at</folder>
    <name>INSPIREGeo-Portal Katalogservice BMLFUW/Österreich</name>
    <url>https://geometadaten.lfrz.at/at.lfrz.discoveryservices/srv/de/csw202</url>
    <filter>
      <ogc:Filter xmlns:ogc="http://www.opengis.net/ogc">
        <ogc:PropertyIsLike escapeChar="\" singleChar="?" wildCard="*">
          <ogc:PropertyName>AnyText</ogc:PropertyName>
          <ogc:Literal>*</ogc:Literal>
        </ogc:PropertyIsLike>
      </ogc:Filter>
    </filter>
  </harvester>
  <harvester>
    <territory>ie</territory>
    <folder>ie</folder>
    <name>Geoportal IE - INSPIREDiscovery Service</name>
    <url>https://www.geoportal.ie/geoportal/csw</url>
  </harvester>
  <harvester>
    <territory>be</territory>
    <folder>be/geobru</folder>
